        openSpan(fo:font-size: 9.0000pt, fo:font-weight: bold, style:font-name: Times New Roman, style:font-pitch: variable)
          insertText(text: Satzung der Vereinigung mittelfränkischer Schülerinnen und Schüler e.V.)
        closeSpan()
      closeParagraph()
      openParagraph(fo:orphans: 2, fo:text-align: center, fo:widows: 2)
        openSpan(fo:font-size: 9.0000pt, fo:font-weight: bold, style:font-name: Times New Roman, style:font-pitch: variable)
        closeSpan()
      closeParagraph()
      openParagraph(fo:orphans: 2, fo:text-align: justify, fo:text-align-last: default, fo:widows: 2)
        openSpan(fo:font-size: 9.0000pt, fo:font-weight: bold, style:font-name: Times New Roman, style:font-pitch: variable)
          insertText(text: § 1 Name und Sitz des Vereins)
        closeSpan()
      closeParagraph()
      openParagraph(fo:margin-left: 7.1000pt, fo:margin-right: 0.0000pt, fo:orphans: 2, fo:text-align: justify, fo:text-align-last: default, fo:text-indent: -7.1000pt, fo:widows: 2, style:auto-text-indent: false, style:tab-stops: ((style:leader-style: solid, style:leader-text:  , style:position: 0.0000pt, style:type: left)))
        openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
          insertText(text: a))
          insertTab()
          insertText(text: Der Verein führt den Namen „Vereinigung mittelfränkischer Schülerinnen und Schüler e.V.“ (VMS e.V.))
        closeSpan()
      closeParagraph()
      openParagraph(fo:margin-left: 7.1000pt, fo:margin-right: 0.0000pt, fo:orphans: 2, fo:text-align: justify, fo:text-align-last: default, fo:text-indent: -7.1000pt, fo:widows: 2, style:auto-text-indent: false, style:tab-stops: ((style:leader-style: solid, style:leader-text:  , style:position: 0.0000pt, style:type: left)))
        openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
          insertText(text: b))
          insertTab()
          insertText(text: Der Verein hat seinen Sitz in Nürnberg und ist in das Vereinsregister eingetragen.)
        closeSpan()
      closeParagraph()
      openParagraph(fo:margin-bottom: 0.0000pt, fo:margin-top: 6.0000pt, fo:orphans: 2, fo:text-align: justify, fo:text-align-last: default, fo:widows: 2)
        openSpan(fo:font-size: 9.0000pt, fo:font-weight: bold, style:font-name: Times New Roman, style:font-pitch: variable)
          insertText(text: § 2 Zweck des Vereins)
        closeSpan()
      closeParagraph()
      openOrderedListLevel(fo:margin-left: 57.3000pt, fo:text-align: left, fo:text-indent: -18.0000pt, librevenge:level: 1, librevenge:list-id: 1, style:num-format: a, text:start-value: 1)
        openListElement(fo:margin-left: 7.1000pt, fo:margin-right: 0.0000pt, fo:orphans: 2, fo:text-align: justify, fo:text-align-last: default, fo:text-indent: -7.1000pt, fo:widows: 2, style:auto-text-indent: false, style:tab-stops: ((style:leader-style: solid, style:leader-text:  , style:position: 0.0000pt, style:type: left)))
          openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
            insertText(text: Der Verein versteht sich als eine Initiative der Jugendarbeit.)
          closeSpan()
        closeListElement()
        openListElement(fo:margin-left: 7.1000pt, fo:margin-right: 0.0000pt, fo:orphans: 2, fo:text-align: justify, fo:text-align-last: default, fo:text-indent: -7.1000pt, fo:widows: 2, style:auto-text-indent: false, style:tab-stops: ((style:leader-style: solid, style:leader-text:  , style:position: 0.0000pt, style:type: left)))
          openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
            insertText(text: Er hat die Aufgabe, im Bezirk Mittelfranken die Zusammenarbeit von Schule und Jugendarbeit zu fördern sowie die Interessenvertretung und Mitsprache der Schülerinnen und Schüler bei schulischen und außerschulischen Belangen zu verbessern. Er unterstützt die gewählten Schü)
          closeSpan()
          openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
            insertText(text: ­lervertreterinnen und Schülervertreter sowie Schülerinnen und Schüler, die Schule aktiv mit­gestalten wollen und unterhält Kontakte zu den im schulischen bzw. schulpolitischen Bereich und in der Jugendarbeit Tätigen. )
          closeSpan()
        closeListElement()
        openListElement(fo:margin-left: 7.1000pt, fo:margin-right: 0.0000pt, fo:orphans: 2, fo:text-align: justify, fo:text-align-last: default, fo:text-indent: -7.1000pt, fo:widows: 2, style:auto-text-indent: false, style:tab-stops: ((style:leader-style: solid, style:leader-text:  , style:position: 0.0000pt, style:type: left)))
          openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
            insertText(text: Er vertritt die jugend- und bildungspolitischen Interessen seiner Mitglieder und versucht durch Treffen, Tagungen, Seminare und Freizeiten Jugendliche von verschiedenen Schularten zu)
          closeSpan()
          openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
            insertText(text: ­sammenzubringen, um sie für SchülerInnen- und Jugendarbeit zu interessieren und zum Engage­ment und gemeinsamen Eintreten für ihre Be­lange zu motivieren.)
          closeSpan()
        closeListElement()
        openListElement(fo:margin-left: 7.1000pt, fo:margin-right: 0.0000pt, fo:orphans: 2, fo:text-align: justify, fo:text-align-last: default, fo:text-indent: -7.1000pt, fo:widows: 2, style:auto-text-indent: false, style:tab-stops: ((style:leader-style: solid, style:leader-text:  , style:position: 0.0000pt, style:type: left)))
          openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
            insertText(text: Der Verein führt die ihm zur Erreichung und Umsetzung der Vereinszwecke geeignet erschei)
          closeSpan()
          openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
            insertText(text: ­nenden Maßnahmen durch; er ist überparteilich, unabhängig und konfessionell nicht gebunden.)
          closeSpan()
        closeListElement()
      closeOrderedListLevel()
      openParagraph(fo:margin-bottom: 0.0000pt, fo:margin-top: 6.0000pt, fo:orphans: 2, fo:text-align: justify, fo:text-align-last: default, fo:widows: 2)
        openSpan(fo:font-size: 9.0000pt, fo:font-weight: bold, style:font-name: Times New Roman, style:font-pitch: variable)
          insertText(text: § 3 Gemeinnützigkeit)
        closeSpan()
      closeParagraph()
      openParagraph(fo:orphans: 2, fo:text-align: justify, fo:text-align-last: default, fo:widows: 2)
        openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
          insertText(text: Der Verein verfolgt ausschließlich und unmittelbar gemeinnützige Zwecke im Sinne des Abschnitts „Steuerbegünstigte Zwecke“ der Abgabenordnung.)
        closeSpan()
      closeParagraph()
      openParagraph(fo:orphans: 2, fo:text-align: justify, fo:text-align-last: default, fo:widows: 2)
        openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
          insertText(text: Der Verein ist selbstlos tätig; er verfolgt nicht in erster Linie eigenwirtschaftliche Zwecke. Mittel des Vereins dürfen nur für satzungsmäßige Zwecke verwendet werden. Die Mitglieder erhalten keine Zuwendungen aus Mitteln des Vereins. Es darf keine Person durch Ausgaben, die dem Zweck des Vereins fremd sind, oder durch unverhältnismäßig hohe Vergütungen begünstigt werden.)
        closeSpan()
      closeParagraph()
      openParagraph(fo:margin-bottom: 0.0000pt, fo:margin-top: 6.0000pt, fo:orphans: 2, fo:text-align: justify, fo:text-align-last: default, fo:widows: 2)
        openSpan(fo:font-size: 9.0000pt, fo:font-weight: bold, style:font-name: Times New Roman, style:font-pitch: variable)
          insertText(text: § 4 Geschäftsjahr)
        closeSpan()
      closeParagraph()
      openParagraph(fo:orphans: 2, fo:text-align: justify, fo:text-align-last: default, fo:widows: 2)
        openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
          insertText(text: Das Geschäftsjahr beginnt am 1. September und endet am 31. August des folgenden Kalenderjah)
        closeSpan()
        openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
          insertText(text: ­res.)
        closeSpan()
      closeParagraph()
      openParagraph(fo:margin-bottom: 0.0000pt, fo:margin-top: 6.0000pt, fo:orphans: 2, fo:text-align: justify, fo:text-align-last: default, fo:widows: 2)
        openSpan(fo:font-size: 9.0000pt, fo:font-weight: bold, style:font-name: Times New Roman, style:font-pitch: variable)
          insertText(text: § 5 Mitgliedschaft)
        closeSpan()
      closeParagraph()
      openOrderedListLevel(fo:margin-left: 57.3000pt, fo:text-align: left, fo:text-indent: -18.0000pt, librevenge:level: 1, librevenge:list-id: 1, style:num-format: a, text:start-value: 1)
        openListElement(fo:margin-left: 7.1000pt, fo:margin-right: 0.0000pt, fo:orphans: 2, fo:text-align: justify, fo:text-align-last: default, fo:text-indent: -7.1000pt, fo:widows: 2, style:auto-text-indent: false, style:tab-stops: ((style:leader-style: solid, style:leader-text:  , style:position: 0.0000pt, style:type: left)))
          openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
            insertText(text: Mitglied des Vereins kann jede Schülerin und jeder Schüler einer mittelfränkischen Schule werden.)
          closeSpan()
        closeListElement()
        openListElement(fo:margin-left: 7.1000pt, fo:margin-right: 0.0000pt, fo:orphans: 2, fo:text-align: justify, fo:text-align-last: default, fo:text-indent: -7.1000pt, fo:widows: 2, style:auto-text-indent: false, style:tab-stops: ((style:leader-style: solid, style:leader-text:  , style:position: 0.0000pt, style:type: left)))
          openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
            insertText(text: Förderndes Mitglied kann jede Person werden. Fördernde Mitglieder haben kein Stimmrecht in der Mitgliederversammlung und können nicht in den Vorstand gewählt werden. Rede- und An)
          closeSpan()
          openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
            insertText(text: ­tragsrecht bleiben unberührt.)
          closeSpan()
        closeListElement()
        openListElement(fo:margin-left: 7.1000pt, fo:margin-right: 0.0000pt, fo:orphans: 2, fo:text-align: justify, fo:text-align-last: default, fo:text-indent: -7.1000pt, fo:widows: 2, style:auto-text-indent: false, style:tab-stops: ((style:leader-style: solid, style:leader-text:  , style:position: 0.0000pt, style:type: left)))
          openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
            insertText(text: Über den schriftlichen Mitgliedsantrag entschei)
          closeSpan()
          openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
            insertText(text: ­det der Vorstand. Die Mitgliedschaft wird durch Eintragung in das Vereinsbuch erworben.)
          closeSpan()
        closeListElement()
        openListElement(fo:margin-left: 7.1000pt, fo:margin-right: 0.0000pt, fo:orphans: 2, fo:text-align: justify, fo:text-align-last: default, fo:text-indent: -7.1000pt, fo:widows: 2, style:auto-text-indent: false, style:tab-stops: ((style:leader-style: solid, style:leader-text:  , style:position: 0.0000pt, style:type: left)))
          openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
            insertText(text: Die Mitgliedschaft endet)
          closeSpan()
        closeListElement()
      closeOrderedListLevel()
      openParagraph(fo:margin-left: 21.3000pt, fo:margin-right: 0.0000pt, fo:orphans: 2, fo:text-align: justify, fo:text-align-last: default, fo:text-indent: -7.1000pt, fo:widows: 2, style:auto-text-indent: false, style:tab-stops: ((style:leader-style: solid, style:leader-text:  , style:position: 0.0000pt, style:type: left)))
        openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
          insertText(text: 1) durch schriftliche, an den Verein gerichtete Austrittserklärung zum Ende eines Ge)
        closeSpan()
        openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
          insertText(text: ­schäftsjahres)
        closeSpan()
      closeParagraph()
      openParagraph(fo:margin-left: 21.3000pt, fo:margin-right: 0.0000pt, fo:orphans: 2, fo:text-align: justify, fo:text-align-last: default, fo:text-indent: -7.1000pt, fo:widows: 2, style:auto-text-indent: false, style:tab-stops: ((style:leader-style: solid, style:leader-text:  , style:position: 0.0000pt, style:type: left)))
        openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
          insertText(text: 2) durch Ausschluß des Mitglieds aus dem Verein)
        closeSpan()
      closeParagraph()
      openParagraph(fo:margin-left: 21.3000pt, fo:margin-right: 0.0000pt, fo:orphans: 2, fo:text-align: justify, fo:text-align-last: default, fo:text-indent: -7.1000pt, fo:widows: 2, style:auto-text-indent: false, style:tab-stops: ((style:leader-style: solid, style:leader-text:  , style:position: 0.0000pt, style:type: left)))
        openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
          insertText(text: 3) mit dem Tod des Mitglieds)
        closeSpan()
      closeParagraph()
      openOrderedListLevel(fo:margin-left: 36.0000pt, fo:text-align: left, fo:text-indent: -18.0000pt, librevenge:level: 1, librevenge:list-id: 3, style:num-format: a, text:start-value: 5)
        openListElement(fo:margin-left: 7.1000pt, fo:margin-right: 0.0000pt, fo:orphans: 2, fo:text-align: justify, fo:text-align-last: default, fo:text-indent: -7.1000pt, fo:widows: 2, style:auto-text-indent: false, style:tab-stops: ((style:leader-style: solid, style:leader-text:  , style:position: 0.0000pt, style:type: left), (style:leader-style: solid, style:leader-text:  , style:position: 35.4500pt, style:type: left)))
          openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
            insertText(text: Nach Ausscheiden aus der Schule erwirbt das Mitglied automatisch die Fördermitgliedschaft. )
          closeSpan()
        closeListElement()
        openListElement(fo:margin-left: 7.1000pt, fo:margin-right: 0.0000pt, fo:orphans: 2, fo:text-align: justify, fo:text-align-last: default, fo:text-indent: -7.1000pt, fo:widows: 2, style:auto-text-indent: false, style:tab-stops: ((style:leader-style: solid, style:leader-text:  , style:position: 0.0000pt, style:type: left), (style:leader-style: solid, style:leader-text:  , style:position: 35.4500pt, style:type: left)))
          openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
            insertText(text: Ein Mitglied, das in erheblichem Maße gegen die Vereinsinteressen verstoßen hat, kann durch Beschluß der Mitgliederversammlung aus dem Verein ausgeschlossen werden. Vor dem Aus)
          closeSpan()
          openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
            insertText(text: ­schluß wird dem betroffenen Mitglied Gelegen­heit gegeben, sich persönlich oder schriftlich zu äußern.)
          closeSpan()
        closeListElement()
      closeOrderedListLevel()
      openParagraph(fo:margin-bottom: 0.0000pt, fo:margin-top: 6.0000pt, fo:orphans: 2, fo:text-align: justify, fo:text-align-last: default, fo:widows: 2)
        openSpan(fo:font-size: 9.0000pt, fo:font-weight: bold, style:font-name: Times New Roman, style:font-pitch: variable)
          insertText(text: § 6 Organe)
        closeSpan()
      closeParagraph()
      openParagraph(fo:orphans: 2, fo:text-align: justify, fo:text-align-last: default, fo:widows: 2)
        openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
          insertText(text: Die Organe des Vereins sind)
        closeSpan()
      closeParagraph()
      openOrderedListLevel(fo:margin-left: 36.0000pt, fo:text-align: left, fo:text-indent: -18.0000pt, librevenge:level: 1, librevenge:list-id: 3, style:num-format: a, text:start-value: 5)
        openListElement(fo:margin-left: 14.2000pt, fo:margin-right: 0.0000pt, fo:orphans: 2, fo:text-align: justify, fo:text-align-last: default, fo:text-indent: -7.1000pt, fo:widows: 2, style:auto-text-indent: false, style:tab-stops: ())
          openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
            insertText(text: der Vorstand)
          closeSpan()
        closeListElement()
        openListElement(fo:margin-left: 14.2000pt, fo:margin-right: 0.0000pt, fo:orphans: 2, fo:text-align: justify, fo:text-align-last: default, fo:text-indent: -7.1000pt, fo:widows: 2, style:auto-text-indent: false, style:tab-stops: ())
          openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
            insertText(text: der erweiterte Vorstand)
          closeSpan()
        closeListElement()
        openListElement(fo:margin-left: 14.2000pt, fo:margin-right: 0.0000pt, fo:orphans: 2, fo:text-align: justify, fo:text-align-last: default, fo:text-indent: -7.1000pt, fo:widows: 2, style:auto-text-indent: false, style:tab-stops: ())
          openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
            insertText(text: die Mitgliederversammlung)
          closeSpan()
        closeListElement()
        openListElement(fo:margin-left: 14.2000pt, fo:margin-right: 0.0000pt, fo:orphans: 2, fo:text-align: justify, fo:text-align-last: default, fo:text-indent: -7.1000pt, fo:widows: 2, style:auto-text-indent: false, style:tab-stops: ())
          openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
            insertText(text: die Ausschüsse und Regionalgruppen)
          closeSpan()
        closeListElement()
      closeOrderedListLevel()
      openParagraph(fo:orphans: 2, fo:text-align: justify, fo:text-align-last: default, fo:widows: 2)
        openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
          insertText(text: Sie geben sich jeweils eine Geschäftsordnung und führen über ihre Sitzungen Protokoll, das von dem Sitzungsleiter / der Sitzungsleiterin und dem Protokollführer / der Protokollführerin zu unter)
        closeSpan()
        openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
          insertText(text: ­zeichnen ist.)
        closeSpan()
      closeParagraph()
      openParagraph(fo:margin-bottom: 0.0000pt, fo:margin-top: 6.0000pt, fo:orphans: 2, fo:text-align: justify, fo:text-align-last: default, fo:widows: 2)
        openSpan(fo:font-size: 9.0000pt, fo:font-weight: bold, style:font-name: Times New Roman, style:font-pitch: variable)
          insertText(text: § 7 Der Vorstand)
        closeSpan()
      closeParagraph()
      openOrderedListLevel(fo:margin-left: 57.3000pt, fo:text-align: left, fo:text-indent: -18.0000pt, librevenge:level: 1, librevenge:list-id: 1, style:num-format: a, text:start-value: 1)
        openListElement(fo:margin-left: 7.1000pt, fo:margin-right: 0.0000pt, fo:orphans: 2, fo:text-align: justify, fo:text-align-last: default, fo:text-indent: -7.1000pt, fo:widows: 2, style:auto-text-indent: false, style:tab-stops: ((style:leader-style: solid, style:leader-text:  , style:position: 0.0000pt, style:type: left)))
          openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
            insertText(text: Der Vorstand des Vereins besteht aus drei gleichberechtigten Vorsitzenden. Der Verein wird gerichtlich und außergerichtlich durch zwei Vorstandsmitglieder vertreten.)
          closeSpan()
        closeListElement()
        openListElement(fo:margin-left: 7.1000pt, fo:margin-right: 0.0000pt, fo:orphans: 2, fo:text-align: justify, fo:text-align-last: default, fo:text-indent: -7.1000pt, fo:widows: 2, style:auto-text-indent: false, style:tab-stops: ((style:leader-style: solid, style:leader-text:  , style:position: 0.0000pt, style:type: left)))
          openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
            insertText(text: Der Vorstand ist an die Beschlüsse der Mitglie)
          closeSpan()
          openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
            insertText(text: ­derversammlung gebunden. Er legt über seine Tätigkeit der Mitgliederversammlung Rechen­schaft ab.)
          closeSpan()
        closeListElement()
        openListElement(fo:margin-left: 7.1000pt, fo:margin-right: 0.0000pt, fo:orphans: 2, fo:text-align: justify, fo:text-align-last: default, fo:text-indent: -7.1000pt, fo:widows: 2, style:auto-text-indent: false, style:tab-stops: ((style:leader-style: solid, style:leader-text:  , style:position: 0.0000pt, style:type: left)))
          openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
            insertText(text: Der Vorstand wird von der Mitgliederver)
          closeSpan()
          openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
            insertText(text: ­sammlung aus deren Mitte für die Dauer eines Jahres gewählt. Er bleibt so lange im Amt, bis eine Neuwahl erfolgt. Scheidet ein Mitglied des Vorstandes während einer Amtsperiode aus, so wählt die Mitgliederversammlung ein Ersatzmit­glied für den Rest der Amtsdauer ausgeschie­denen Vorstandsmitgliedes.)
          closeSpan()
        closeListElement()
        openListElement(fo:margin-left: 7.1000pt, fo:margin-right: 0.0000pt, fo:orphans: 2, fo:text-align: justify, fo:text-align-last: default, fo:text-indent: -7.1000pt, fo:widows: 2, style:auto-text-indent: false, style:tab-stops: ((style:leader-style: solid, style:leader-text:  , style:position: 0.0000pt, style:type: left)))
          openSpan(fo:font-size: 8.0000pt, style:font-name: Times New Roman, style:font-pitch: variable)
